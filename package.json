{
	"name": "mf-shell",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --configuration local",
		"build:dev": "ng build --configuration dev --base-href ./",
		"build:cert": "ng build --configuration cert --base-href ./",
		"build:prod": "ng build --configuration prod --base-href ./",
		"watch": "ng build --watch --configuration dev",
		"test": "ng test",
		"run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
		"lint": "ng lint",
		"format": "prettier --write \"./src/**/*.{ts,json,html}\""
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^16.2.0",
		"@angular/common": "^16.2.0",
		"@angular/compiler": "^16.2.0",
		"@angular/core": "^16.2.0",
		"@angular/forms": "^16.2.0",
		"@angular/platform-browser": "^16.2.0",
		"@angular/platform-browser-dynamic": "^16.2.0",
		"@angular/router": "^16.2.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.13.0"
	},
	"devDependencies": {
		"@angular-architects/module-federation": "^16.0.4",
		"@angular-devkit/build-angular": "^16.2.2",
		"@angular-eslint/builder": "16.2.0",
		"@angular-eslint/eslint-plugin": "16.2.0",
		"@angular-eslint/eslint-plugin-template": "16.2.0",
		"@angular-eslint/schematics": "16.2.0",
		"@angular-eslint/template-parser": "16.2.0",
		"@angular/cli": "~16.2.2",
		"@angular/compiler-cli": "^16.2.0",
		"@types/jasmine": "~4.3.0",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"jasmine-core": "~4.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"ngx-build-plus": "^16.0.0",
		"postcss": "^8.4.30",
		"prettier": "^3.0.3",
		"typescript": "~5.1.3"
	}
